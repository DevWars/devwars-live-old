{
  "name": "devwars-editor",
  "version": "0.1.0",
  "description": "Text editor for DevWars",
  "main": "src/server/index.js",
  "author": "Kim Simonsen",
  "license": "MIT",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "npm run build:client",
    "build:client": "webpack --config webpack.prod.js",
    "watch": "concurrently -kr \"npm run watch:client\" \"npm run watch:server\"",
    "watch:client": "webpack -w --config webpack.base.js",
    "watch:server": "onchange -i \"src/server/**/*\" -- node src/server/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.34",
    "@babel/polyfill": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "monaco-editor": "^0.10.0",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vuex": "3.0.1",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "express": "^4.15.4",
    "socket.io": "^2.0.3"
  },
  "browserslist": [
    "chrome >= 61",
    "edge >= 16",
    "firefox >= 56",
    "safari >= 11"
  ]
}
